{
    "name": "jlog-gcloud-dest",
    "version": "0.3.3",
    "main": "lib/index.js",
    "author": "marcos.lin@farport.co",
    "license": "MIT",
    "types": "lib/index",
    "scripts": {
        "build:clean": "rm -f tsconfig*.tsbuildinfo || true && rm -rf lib",
        "build": "yarn lint && yarn build:clean && yarn build:ts && yarn build:alias && yarn test:lib",
        "build:ts": "tsc --project ./tsconfig.build.json",
        "build:alias": "tsc-alias --project ./tsconfig.build.json",
        "lint:format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "lint:ts": "eslint \"{src,test}/**/*.ts\" --fix",
        "lint": "yarn lint:ts && yarn lint:format",
        "test:unit": "jest --config ./jest.config.ts",
        "test:lib": "jest --config ./jest.config.lib.ts",
        "test:debug": "jest --config ./jest.config.ts --runInBand --bail",
        "test": "yarn test:unit"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.2.0",
        "@eslint/js": "^9.18.0",
        "@types/jest": "^30.0.0",
        "@types/node": "^24.1.0",
        "@typescript-eslint/eslint-plugin": "^8.38.0",
        "@typescript-eslint/parser": "^8.38.0",
        "eslint": "^9.32.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-prettier": "^5.2.2",
        "globals": "^15.15.0",
        "jest": "^30.0.4",
        "prettier": "^3.6.2",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.1",
        "tsc-alias": "^1.8.16",
        "tsconfig-paths": "^4.1.2",
        "typedoc": "^0.28.9",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.20.0"
    },
    "dependencies": {
        "jlog-facade": "^0.10.1"
    },
    "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
